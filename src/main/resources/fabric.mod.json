    "issues": "https://github.com/OverlordsIII/ConfiguredKeepInventory/issues"
  },
  "license": "MIT",
  "icon": "assets/inventory/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.overlordsiii.ConfiguredKeepInventory"
    ],
    "modmenu": [
      "io.github.overlordsiii.integration.ModMenuIntegration"
    ]
  },
  "mixins": [
    "inventory.config.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.1+build.205",
